t('sm-number', () => {\n    const wrapper = mount(() => <Col sm={12} />)\n    expect(wrapper.classes()).toContain('el-col-sm-12')\n  })\n\n  test('xs-number', () => {\n    const wrapper = mount(() => <Col xs={12} />)\n    expect(wrapper.classes()).toContain('el-col-xs-12')\n  })\n\n  test('md-number', () => {\n    const wrapper = mount(() => <Col md={12} />)\n    expect(wrapper.classes()).toContain('el-col-md-12')\n  })\n\n  test('lg-number', () => {\n    const wrapper = mount(() => <Col lg={12} />)\n    expect(wrapper.classes()).toContain('el-col-lg-12')\n  })\n\n  test('xl-number', () => {\n    const wrapper = mount(() => <Col xl={12} />)\n    expect(wrapper.classes()).toContain('el-col-xl-12')\n  })\n\n  test('xs-object', () => {\n    const wrapper = mount(() => (\n      <Col xs={{ span: 4, offset: 4, pull: 4, push: 4 }} />\n    ))\n    expect(wrapper.classes()).toContain('el-col-xs-4')\n    expect(wrapper.classes()).toContain('el-col-xs-offset-4')\n    expect(wrapper.classes()).toContain('el-col-xs-pull-4')\n    expect(wrapper.classes()).toContain('el-col-xs-push-4')\n  })\n\n  test('sm-object', () => {\n    const wrapper = mount(() => <Col sm={{ span: 4, offset: 4 }} />)\n    expect(wrapper.classes()).toContain('el-col-sm-4')\n    expect(wrapper.classes()).toContain('el-col-sm-offset-4')\n  })\n\n  test('md-object', () => {\n    const wrapper = mount(() => <Col md={{ span: 4, offset: 4 }} />)\n    expect(wrapper.classes()).toContain('el-col-md-4')\n    expect(wrapper.classes()).toContain('el-col-md-offset-4')\n  })\n\n  test('lg-object', () => {\n    const wrapper = mount(() => <Col lg={{ span: 4, offset: 4 }} />)\n    expect(wrapper.classes()).toContain('el-col-lg-4')\n    expect(wrapper.classes()).toContain('el-col-lg-offset-4')\n  })\n\n  test('xl-object', () => {\n    const wrapper = mount(() => <Col xl={{ span: 4, offset: 4 }} />)\n    expect(wrapper.classes()).toContain('el-col-xl-4')\n    expect(wrapper.classes()).toContain('el-col-xl-offset-4')\n  })\n\n  test('gutter class test', () => {\n    const wrapper = mount({\n      setup() {\n        return () => (\n          <Row gutter={1}>\n            <Col ref=\"col\" xl={{ span: 4, offset: 4 }} />\n          </Row>\n        )\n      },\n    })\n    // const colElm = wrapper.findComponent({ ref: 'col' }).element as HTMLElement\n    // console.log(colElm.style)\n\n    const colElm = wrapper.findComponent({ ref: 'col' }).classes()\n    expect(colElm.includes('is-guttered')).toBe(true)\n  })\n\n  test('col gutter style test', () => {\n    const wrapper = mount({\n      setup() {\n        return () => (\n          <Row gutter={10}>\n            <Col ref=\"col\" xl={{ span: 4, offset: 4 }} />\n          </Row>\n        )\n      },\n    })\n    const colElm = wrapper.findComponent({ ref: 'col' }).element as HTMLElement\n\n    expect(\n      colElm.style.paddingLeft == colElm.style.paddingRight &&\n        colElm.style.paddingLeft == '5px'\n    ).toBe(true)\n  })\n\n  test('row gutter style test', () => {\n    const wrapper = mount(() => <Row gutter={50} />)\n    const el = wrapper.element as HTMLElement\n    expect(\n      el.style.marginLeft == el.style.marginRight &&\n        el.style.marginLeft == '-25px'\n    ).toBe(true)\n  })\n})\n","/wanxin/github/element3/packages/components/col/index.ts",[],[],"/wanxin/github/element3/packages/components/col/src/col.ts",[],[],"/wanxin/github/element3/packages/components/col/src/col.vue",["907","908"],[],"<script setup lang=\"ts\">\nimport { computed, inject } from 'vue'\nimport type { CSSProperties } from 'vue'\nimport { colProps } from './col'\nimport { useNamespace } from '@element3/hooks'\nimport { rowContextKey } from '@element3/tokens'\n\nconst { gutter } = inject(rowContextKey, { gutter: computed(() => 0) })\n\ndefineOptions({\n  name: 'ElCol',\n})\n\ndefineEmits({})\n\nconst props = defineProps({ ...colProps })\n\nconst ns = useNamespace('col')\n\nconst style = computed(() => {\n  const styles: CSSProperties = {}\n\n  if (gutter.value) {\n    styles.paddingLeft = styles.paddingRight = `${gutter.value / 2}px`\n  }\n\n  return styles\n})\n\nconst classes = computed(() => {\n  const classes: string[] = []\n  const pos = ['span', 'offset', 'pull', 'push'] as const\n\n  pos.forEach((prop) => {\n    if (prop == 'span') {\n      classes.push(ns.b(`${props[prop]}`))\n    } else {\n      classes.push(ns.b(`${prop}-${props[prop]}`))\n    }\n  })\n\n  const sizes = ['xs', 'sm', 'md', 'lg', 'xl'] as const\n\n  sizes.forEach((size) => {\n    if (typeof props[size] == 'number') {\n      classes.push(ns.b(`${size}-${props[size]}`))\n    } else if (typeof props[size] == 'object' && props[size] != null) {\n      Object.entries(props[size]).forEach(([key, val]) => {\n        classes.push(\n          key !== 'span'\n            ? ns.b(`${size}-${key}-${val}`)\n            : ns.b(`${size}-${val}`)\n        )\n      })\n    }\n  })\n\n  if (gutter.value) {\n    classes.push(ns.is('guttered'))\n  }\n\n  return classes\n})\n</script>\n\n<template>\n  <component :is=\"tag\" :class=\"[ns.b(), classes]\" :style=\"style\"\n    ><slot\n  /></component>\n</template>\n<style scoped></style>\n","/wanxin/github/element3/packages/components/col/style/css.ts",[],[],"/wanxin/github/element3/packages/components/col/style/index.ts",[],[],"/wanxin/github/element3/packages/components/container/__test__/container.test.tsx",["909"],[],"import { mount } from '@vue/test-utils'\nimport { test, describe, expect } from 'vitest'\nimport Header from '../src/header.vue'\nimport Aside from '../src/aside.vue'\nimport Footer from '../src/footer.vue'\n\ndescribe('Header.vue', () => {\n  test('Header render test', async () => {\n    const wrapper = mount(() => <Header />)\n    expect(wrapper.classes()).toContain('el-header')\n  })\n\n  test('Header height', async () => {\n    const wrapper = mount(() => <Header height=\"500px\" />)\n    const vm = wrapper.vm\n    expect(\n      getComputedStyle(vm.$el).getPropertyValue('--el-header-height')\n    ).toEqual('500px')\n  })\n})\n\ndescribe('Footer.vue', () => {\n  test('Footer render test', async () => {\n    const wrapper = mount(() => <Footer />)\n    expect(wrapper.classes()).toContain('el-footer')\n  })\n\n  test('Footer height', async () => {\n    const wrapper = mount(() => <Footer height=\"500px\" />)\n    const vm = wrapper.vm\n    expect(\n      getComputedStyle(vm.$el).getPropertyValue('--el-footer-height')\n    ).toEqual('500px')\n  })\n})\n\ndescribe('Aside.vue', () => {\n  test('Aside render test', async () => {\n    const wrapper = mount(() => <Aside />)\n    expect(wrapper.classes()).toContain('el-aside')\n  })\n\n  test('Aside height', async () => {\n    const wrapper = mount(() => <Aside width=\"500px\" />)\n    const vm = wrapper.vm\n    expect(\n      getComputedStyle(vm.$el).getPropertyValue('--el-aside-width')\n    ).toEqual('500px')\n  })\n})\n","/wanxin/github/element3/packages/components/container/index.ts",[],[],"/wanxin/github/element3/packages/components/container/src/aside.vue",["910"],[],"<script setup lang=\"ts\">\nimport { useNamespace } from '@element3/hooks'\nimport { computed } from 'vue'\nimport type { CSSProperties } from 'vue'\nconst ns = useNamespace('aside')\n\ndefineOptions({\n  name: 'ElAside',\n})\n\nconst props = defineProps({\n  width: {\n    type: String,\n    default: null,\n  },\n})\n\nconst style = computed(\n  () =>\n    (props.width ? ns.cssVarBlock({ width: props.width }) : {}) as CSSProperties\n)\n</script>\n<template>\n  <aside :class=\"ns.b()\" :style=\"style\">\n    <slot />\n  </aside>\n</template>\n","/wanxin/github/element3/packages/components/container/src/container.vue",["911"],[],"<script setup lang=\"ts\">\nimport { computed, useSlots } from 'vue'\nimport type { Component, VNode } from 'vue'\nimport { useNamespace } from '@element3/hooks'\n\nconst slots = useSlots()\n\ndefineOptions({\n  name: 'ElContainer',\n})\n\nconst props = defineProps({\n  direction: {\n    type: String,\n  },\n})\nconst ns = useNamespace('container')\n\nconst isVertical = computed(() => {\n  if (props.direction === 'vertical') {\n    return true\n  } else if (props.direction === 'horizontal') {\n    return false\n  }\n  if (slots && slots.default) {\n    const vNodes: VNode[] = slots.default()\n    return vNodes.some((vNode) => {\n      const tag = (vNode.type as Component).name\n      return tag === 'ElHeader' || tag === 'ElFooter'\n    })\n  } else {\n    return false\n  }\n})\n</script>\n<template>\n  <section :class=\"[ns.b(), ns.is('vertical', isVertical)]\">\n    <slot />\n  </section>\n</template>\n","/wanxin/github/element3/packages/components/container/src/footer.vue",["912"],[],"<script setup lang=\"ts\">\nimport { useNamespace } from '@element3/hooks'\nimport { computed } from 'vue'\nimport type { CSSProperties } from 'vue'\nconst ns = useNamespace('footer')\n\ndefineOptions({\n  name: 'ElFooter',\n})\n\nconst props = defineProps({\n  height: {\n    type: String,\n    default: null,\n  },\n})\n\nconst style = computed(\n  () =>\n    (props.height\n      ? ns.cssVarBlock({ height: props.height })\n      : {}) as CSSProperties\n)\n</script>\n<template>\n  <footer :class=\"ns.b()\" :style=\"style\">\n    <slot />\n  </footer>\n</template>\n","/wanxin/github/element3/packages/components/container/src/header.vue",["913"],[],"<script setup lang=\"ts\">\nimport { computed } from 'vue'\nimport type { CSSProperties } from 'vue'\nimport { useNamespace } from '@element3/hooks'\nconst ns = useNamespace('header')\ndefineOptions({\n  name: 'ElHeader',\n})\n\nconst props = defineProps({\n  height: {\n    type: String,\n    default: '60px',\n  },\n})\n\nconst style = computed(() => {\n  return props.height\n    ? (ns.cssVarBlock({\n        height: props.height,\n      }) as CSSProperties)\n    : {}\n})\n</script>\n\n<template>\n  <header :class=\"ns.b()\" :style=\"style\">\n    <slot />\n  </header>\n</template>\n","/wanxin/github/element3/packages/components/container/src/main.vue",[],[],"/wanxin/github/element3/packages/components/container/style/css.ts",[],[],"/wanxin/github/element3/packages/components/container/style/index.ts",[],[],"/wanxin/github/element3/packages/components/form/__test__/form.test.tsx",[],[],"/wanxin/github/element3/packages/components/form/index.ts",[],[],"/wanxin/github/element3/packages/components/form/src/form.ts",[],[],"/wanxin/github/element3/packages/components/form/src/form.vue",["914"],[],"<script setup lang=\"ts\">\nimport { formProps } from './form'\nimport { useNamespace } from '@element3/hooks'\ndefineOptions({\n  name: 'ElForm',\n})\ndefineProps({ ...formProps })\ndefineEmits({})\nconst ns = useNamespace('form')\n</script>\n\n<template>\n  <div :class=\"ns.b()\"><slot /></div>\n</template>\n<style scoped></style>\n","/wanxin/github/element3/packages/components/form/style/css.ts",[],[],"/wanxin/github/element3/packages/components/form/style/index.ts",[],[],"/wanxin/github/element3/packages/components/grid-layout/__test__/grid-layout.test.tsx",["915","916"],[],"import { describe, expect, it } from 'vitest'\nimport { mount } from '@vue/test-utils'\nimport Layout from '../src/grid-layout.vue'\nimport Item from '../src/grid-layout-item.vue'\n\ndescribe('grid-layout test', () => {\n  it('Grid Layout create', () => {\n    const wrapper = mount(() => <Layout />)\n    expect(wrapper.classes()).toContain('el-grid-layout')\n  })\n\n  it('Grid Layout item create', () => {\n    const wrapper = mount(() => <Item name=\"haha\" />)\n    expect(wrapper.classes()).toContain('el-grid-layout-item')\n  })\n\n  it('Grid Layout item name', () => {\n    const wrapper = mount(() => <Item name=\"haha\" />)\n    let dom = wrapper.find('.el-grid-layout-item')\n    expect(dom.attributes('style')).toContain('grid-area: haha')\n  })\n})\n","/wanxin/github/element3/packages/components/grid-layout/index.ts",[],[],"/wanxin/github/element3/packages/components/grid-layout/src/grid-layout-item.vue",[],[],"/wanxin/github/element3/packages/components/grid-layout/src/grid-layout.vue",["917","918","919","920","921","922","923","924","925","926"],[],"<script setup lang=\"ts\">\nimport { computed, onBeforeUnmount, onMounted, ref } from 'vue'\nimport type { PropType } from 'vue'\nimport { useNamespace } from '@element3/hooks'\n\ntype Target = {\n  [x: string]: any\n  lg?: { value: number; areas: string | never[] }\n  md?: { value: number; areas: string | never[] }\n  sm?: { value: number; areas: string | never[] }\n  xs?: { value: number; areas: string | never[] }\n  xl?: { value: number; areas: string | never[] }\n}\n\ntype GridAreas = string[][]\n\ntype GridReactive = Target[]\n\ndefineOptions({\n  name: 'ElGridLayout',\n})\n\nconst props = defineProps({\n  reactive: {\n    type: Object as PropType<GridReactive>,\n    default: () => [],\n  },\n  columns: {\n    type: String,\n    default: '1fr',\n  },\n  rows: {\n    type: String,\n    default: '1fr',\n  },\n  areas: {\n    type: Object as PropType<GridAreas>,\n    default: () => [],\n  },\n  gap: {\n    type: String,\n    default: '',\n  },\n  height: {\n    type: String,\n    default: '100%',\n  },\n})\n\ndefineEmits({})\n\nconst areas = ref()\n\nconst columns = ref()\n\nconst rows = ref()\n\nconst gap = ref()\n\nconst breakpoint = {\n  lg: {\n    value: 1200,\n    areas: [],\n    columns: '',\n    rows: '',\n    gap: '',\n  },\n  md: {\n    value: 992,\n    areas: [],\n    columns: '',\n    rows: '',\n    gap: '',\n  },\n  sm: {\n    value: 768,\n    areas: [],\n    columns: '',\n    rows: '',\n    gap: '',\n  },\n  xs: {\n    value: 480,\n    areas: [],\n    columns: '',\n    rows: '',\n    gap: '',\n  },\n  xl: {\n    value: 1920,\n    areas: [],\n    columns: '',\n    rows: '',\n    gap: '',\n  },\n}\n\nconst _config = ref()\n\nconst ns = useNamespace('grid-layout')\n\nconst style = computed(() => {\n  return {\n    'grid-template-columns': columns.value,\n    'grid-template-rows': rows.value,\n    'grid-template-areas': areas.value,\n    'grid-gap': gap.value,\n    height: props.height,\n  }\n})\n\nfunction extend(target: Target, ...args: Record<string, any>[]) {\n  for (let i = 0; i < args.length; ++i) {\n    let from = args[i]\n    if (typeof from !== 'object') continue\n    for (let j in from) {\n      if (from.hasOwnProperty(j)) {\n        target[j] = Array.isArray(from[j])\n          ? extend([], target[j], from[j])\n          : typeof from[j] === 'object'\n          ? extend({}, target[j], from[j])\n          : from[j]\n      }\n    }\n  }\n  return target\n}\nfunction init() {\n  _config.value = extend(breakpoint, props.reactive)\n}\n\nfunction areasTransform(data: string[][]) {\n  let res = ''\n  if (Array.isArray(data)) {\n    data.forEach((_s) => {\n      if (Array.isArray(_s)) {\n        res += `\"${_s.join(' ')}\"\\n`\n      }\n    })\n  }\n  return res\n}\n\n// TODO: Not elegant enough rewrite!!\n// fork https://github.com/vueuse/vueuse/blob/main/packages/core/useBreakpoints/index.ts\nfunction getAreas() {\n  const sizes = ['xs', 'sm', 'md', 'lg', 'xl']\n  const _w = window.innerWidth\n  for (let i = 0; i < sizes.length; i++) {\n    const cur = _config.value[sizes[i]]\n    const pre = _config.value[sizes[i - 1]]\n    if (!cur) return\n    if (!pre && _w <= cur.value) {\n      setGridAttributes(cur)\n    } else if (_w <= cur.value && _w > pre.value) {\n      setGridAttributes(cur)\n    }\n  }\n}\n\nfunction setGridAttributes(res: { [x: string]: string[][] }) {\n  Object.entries({\n    columns: columns,\n    rows: rows,\n    gap: gap,\n    areas: areas,\n  }).forEach(([key, val]) => {\n    if (key != 'areas') {\n      val.value = res[key] || props[key as keyof typeof props] || ''\n    } else {\n      val.value =\n        res[key] && res[key].length > 0\n          ? areasTransform(res[key])\n          : areasTransform(props[key]) || ''\n    }\n  })\n}\n\nonMounted(() => {\n  init()\n  getAreas()\n  window.addEventListener('resize', getAreas)\n})\n\nonBeforeUnmount(() => {\n  window.removeEventListener('resize', getAreas)\n})\n</script>\n\n<template>\n  <div :class=\"ns.b()\" :style=\"style\"><slot /></div>\n</template>\n<style scoped></style>\n","/wanxin/github/element3/packages/components/grid-layout/style/css.ts",[],[],"/wanxin/github/element3/packages/components/grid-layout/style/index.ts",[],[],"/wanxin/github/element3/packages/components/index.ts",[],[],"/wanxin/github/element3/packages/components/layout/__test__/layout.test.tsx",[],[],"/wanxin/github/element3/packages/components/layout/index.ts",[],[],"/wanxin/github/element3/packages/components/layout/src/layout-item.vue",["927"],[],"<script setup lang=\"ts\">\nimport { computed, onMounted, ref } from 'vue'\nimport { useNamespace } from '@element3/hooks'\n\ninterface _config {\n  x: string\n  y: string\n  w: string\n  h: string\n}\nconst config = ref<_config>({ x: '0', y: '0', w: '50px', h: '50px' })\n\ndefineOptions({\n  name: 'ElLayoutItem',\n})\n\nconst defaultConfig = {\n  x: '0',\n  y: '0',\n  w: '50px',\n  h: '50px',\n}\n\nconst props = defineProps({\n  option: {\n    type: Object,\n    default: {},\n  },\n})\n\ndefineEmits({})\n\nconst ns = useNamespace('layout-item')\n\nonMounted(() => {\n  config.value = Object.assign(defaultConfig, props.option)\n})\n\nconst style = computed(() => {\n  const { w, h, x, y } = config.value\n  return {\n    width: w,\n    height: h,\n    left: x,\n    top: y,\n  }\n})\n</script>\n\n<template>\n  <div :class=\"ns.b()\" :style=\"style\"><slot /></div>\n</template>\n<style scoped></style>\n","/wanxin/github/element3/packages/components/layout/src/layout.vue",["928"],[],"<script setup lang=\"ts\">\nimport { useNamespace } from '@element3/hooks'\nimport { computed } from 'vue'\n\ndefineOptions({\n  name: 'ElLayout',\n})\n\nconst props = defineProps({\n  width: {\n    type: String,\n    default: '1920px',\n  },\n  height: {\n    type: String,\n    default: '1080px',\n  },\n})\n\ndefineEmits({})\n\nconst ns = useNamespace('layout')\n\nconst style = computed(() => {\n  retur